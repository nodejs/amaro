{
	"$schema": "https://biomejs.dev/schemas/1.8.3/schema.json",
	"organizeImports": {
		"enabled": true
	},
	"linter": {
		"enabled": true,
		"rules": {
			"recommended": true
		}
	},
	"formatter": {
		"enabled": true
	},
	"files": {
		"ignore": [
			"package.json",
			"dist/**",
			"deps/**",
			"lib/**",
			"node_modules/**",
			"test/snapshot/**",
			"test/fixtures/**"
		]
	}
}
